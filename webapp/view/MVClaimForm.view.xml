<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns="sap.uxap" xmlns:m="sap.m" xmlns:f="sap.f" xmlns:layout="sap.ui.layout"
	xmlns:form="sap.ui.layout.form" xmlns:semantic="sap.m.semantic" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="qldh.MV_Claim.controller.MVClaimForm" displayBlock="true">
	<semantic:FullscreenPage>
		<semantic:content>
			<m:ObjectHeader xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core" xmlns:m="sap.m" xmlns:f="sap.f" xmlns:layout="sap.ui.layout"
				xmlns:semantic="sap.m.semantic" xmlns:html="http://www.w3.org/1999/xhtml" xmlns="sap.m" title="Header Text" number="100.00" numberUnit="$"
				id="header1" fullScreenOptimized="true" titleActive="true">
				<statuses>
					<ObjectStatus text="Success" state="Success" id="status1"/>
				</statuses>
				<attributes>
					<ObjectAttribute text="10 units" id="attribute1"/>
				</attributes>
			</m:ObjectHeader>
			<m:IconTabBar id="bar0">
				<m:items>
					<m:IconTabFilter text="Details" icon="sap-icon://form" id="claimTab">
						<m:content>
							<mvc:XMLView viewName='qldh.MV_Claim.view.ClaimForm' type='XML'/>
							<!--<core:Fragment fragmentName="qldh.MV_Claim.view.fragments.ClaimForm" type="XML"/>-->
						</m:content>
					</m:IconTabFilter>
					<m:IconTabFilter text="Attachments" icon="sap-icon://attachment" id="attachTab">
						<core:ComponentContainer id="attachments" class="sapUiLargeMarginTop" width="100%" name="qldh.attachments" component="qldh.attachments"
							propagateModel="true" lifecycle="Application"/>
					</m:IconTabFilter>
					<m:IconTabFilter text="Comments" icon="sap-icon://comment" id="commentsTab"/>
					<m:IconTabFilter text="Process Flow" icon="sap-icon://employee-approvals" id="pFlowTab">
						<core:ComponentContainer width="100%" name="qldh.processflow.ZHR_PROCESSFLOW_EM" component="qldh.processflow.ZHR_PROCESSFLOW_EM"
							propagateModel="true" lifecycle="Application"/></m:IconTabFilter>
				</m:items>
			</m:IconTabBar>
		</semantic:content>
	</semantic:FullscreenPage>
</mvc:View>