{
	"_version": "1.8.0",
	"sap.app": {
		"id": "qldh.MV_Claim",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZHR_MV_ALLOW_CLAIM": {
				"uri": "/sap/opu/odata/sap/ZHR_MV_ALLOW_CLAIM/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			},
			"ZHR_ATTACHMENTS_SRV": {
				"uri": "/sap/opu/odata/sap/ZHR_ATTACHMENTS_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/ZHR_ATTACHMENTS_SRV/metadata.xml"
				}
			},
			"ZHR_PERSONS_EXTN_SRV": {
				"uri": "/sap/opu/odata/sap/ZHR_PERSONS_EXTN_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/ZHR_PERSONS_EXTN_SRV/metadata.xml"
				}
			}
		},
		"crossNavigation": {
			"inbounds": {}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://insurance-car",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "qldh.MV_Claim.view.App",
			"type": "XML"
		},
		"resourceRoots": {
			"qldh.personlookup": "../sap/bc/ui5_ui5/sap/zhr_pers_lookup",
			"qldh.personlookup.ZHR_PERLKUP_SUB": "../sap/bc/ui5_ui5/sap/ZHR_PERLKUP_SUB",
			"qldh.attachments": "../sap/bc/ui5_ui5/sap/zhr_attachments"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"components": {
				"qldh.personlookup": {
					"lazy": true
				},
				"qldh.personlookup.ZHR_PERLKUP_SUB": {
					"lazy": true
				},
					"qldh.attachments": {
					"lazy": false
				}
			},
			"libs": {
				"sap.f": {
					"minVersion": ""
				},
				"sap.m": {},
				"sap.ui.comp": {
					"minVersion": ""
				},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ushell": {
					"minVersion": ""
				},
				"sap.uxap": {
					"minVersion": ""
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "qldh.MV_Claim.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline"
				},
				"dataSource": "ZHR_MV_ALLOW_CLAIM",
				"preload": true
			},
			"Attachments": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZHR_ATTACHMENTS_SRV",
				"preload": true
			},
			"Persons": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Inline"
				},
				"dataSource": "ZHR_PERSONS_EXTN_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "qldh.MV_Claim.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "ClaimInit",
					"pattern": "",
					"target": [
						"ClaimInit"
					]
				},
				{
					"name": "MVClaimForm",
					"pattern": "MVClaimForm",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"MVClaimForm"
					]
				}
			],
			"targets": {
				"MVClaimForm": {
					"viewId": "MVClaimForm",
					"viewType": "XML",
					"viewName": "MVClaimForm",
					"viewLevel": 2
				},
				"ClaimInit": {
					"viewType": "XML",
					"viewName": "ClaimInit",
					"viewLeve": 1,
					"viewLevel": 1
				},
				"ClaimForm": {
					"viewType": "XML",
					"viewName": "ClaimForm"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zhr_mv_claim/webapp",
		"_version": "1.1.0"
	}
}